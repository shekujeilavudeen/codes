public class largestElement {
    public static void main(String[] args) {
        int[] numbers = {12, 34, 56, 78, 80};
        int max = numbers[0];

        for (int num : numbers) {
            if (num > max) {
                max = num;
            }
        }

        System.out.println("The largest element is: " + max);
    }
}
