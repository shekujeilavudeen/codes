package com.java;
public class fizzbuzz{
    public static void main(string[]args){
      int n=20;
      for(int i=1;i<=n;i++){
         if(i%15==0){
        systeam.out.printin("fizz buzz");     
         }else if(i%3==0)}
           systeam .out.printin("fizz");
          }else if(i%5==0){
            systeam.out.printin("buzz");
          }else{
              systeam.out.printin(i);
          }
   }
   
}
}
     
