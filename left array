package com.problems;

public class Arrayleft rotate{
public static void main(String[]args){
    int[]arr={1,2,3,4,5,6};
    int n=3;
    for(int i=0;i<n;i++){
       int last element=arr[arr.length-1];
       for(int j=arr.length-1;j>0;j--){
           arr[j]-arr[j-1];
       }
       arr[0]=last element;
       }
     systeam.out.printin("Array after rotate");
     for(int i=0;i<arr.length;i++){
         systeam.out.print(arr[i])+"");
         
     }}
